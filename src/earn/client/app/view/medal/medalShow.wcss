.new-page{
    background: #f2f2f2;
    background: rgba(34,34,34,0.9);
}
.content{
    width: 100%;
    height:100%;
    margin-top: -88px;
    padding: 0px 0px 70px;
    justify-content: space-between;
}
.medal{
    margin-top: 200px;
    width: 750px;
    height: 750px;
    position: relative;
}
.medal-bgimg{

}
.medal-img{
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%)  translateY(-50%);
}
.medal-title{
    font-size:48px;
    color:#F7CE89;
}
.medal-desc{
    font-size:32px;
    color:#BFAA9A;
    margin-top: 15px;
}
.medal-btn{
    width:500px;
    height:96px;
    background:#4B7EFF;
    border-radius:12px;
    font-size:32px;
    font-weight:500;
    color:#ffffff;
    text-align: center;
    line-height: 96px;
}


.flex-col{
    display: flex;
    flex-direction: column;
    align-items: center;
}
