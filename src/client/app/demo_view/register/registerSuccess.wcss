.title{ 
    color: rgba(255, 255, 255, 1);
    font-size: 32px;
    font-family: PingFangSC-Regular;
}
.success-status-wrap{
    height:174px;
    margin:50px 30px;
    padding-bottom: 20px;
    border-bottom:solid #DBDBE5 1px;
    display:flex;
    flex-direction:column;
    align-items:center;
}
.successIcon{
    width:100px;
}
.successText{
    color: rgba(34, 34, 34, 1);
    font-size: 36px;
    font-family: PingFangSC-Medium;
    font-weight:bold;
    margin-top:16px;
}
.id-wrap{
    width:100%;
    display:flex;
    flex-direction:column;
    align-items:center;
    justify-content:center;
}
.other-text{
    color: rgba(136, 136, 136, 1);
    font-size: 28px;
    font-family: PingFangSC-Regular;
}
.id{  
    color: rgba(34, 34, 34, 1);
    font-size: 64px;
    font-family: PingFangSC-Regular;
}
.goChat{
    margin: 80px 30px;
    height: 100px;
    border-radius: 6px;
    background-color: rgba(49, 141, 230, 1);
    line-height: 100px;
    color: rgba(255, 255, 255, 1);
    font-size: 36px;
    text-align: center;
    font-family: PingFangSC-Regular;
}